{
  "name": "pollen-solutions/kernel",
    "description": "Pollen Kernel Component",
  "homepage": "https://www.presstify.com/pollen-solutions/kernel/",
  "keywords": [
    "pollen-solutions",
    "component",
    "kernel"
  ],
  "time": "2021-05-20",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/installers": "~1.0",
    "pollen-solutions/support": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Kernel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Kernel\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
